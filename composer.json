    "homepage": "https://github.com/mogilvie/GdprBundle",
    "authors": [
        {
            "name": "Mark Ogilvie",
            "email": "mark.ogilvie@ogilvieconsulting.net"
        }
    ],
    "keywords": [
        "symfony gdpr dpia encryption encrypt bundle"
