    ],
    "require": {
        "php": ">=7.1",
        "symfony/console": "^3.2 || ^4.0 || ^5.0",
        "symfony/form": "^3.2 || ^4.0 || ^5.0",
        "symfony/framework-bundle": "^3.2 || ^4.0 || ^5.0",
        "symfony/validator": "^3.2 || ^4.0 || ^5.0" ,
        "symfony/twig-bundle": "^3.2 || ^4.0 || ^5.0",
        "symfony/security-bundle": "^3.2 || ^4.0 || ^5.0",
        "twig/twig": "^2.0",
